

{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "icons": {
    "16": "16icon.png",
    "48": "48icon.png",
    "128": "128icon.png"
  },
  "browser_action": {
    "default_popup": "html/index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
